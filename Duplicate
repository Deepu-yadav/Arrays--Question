public class Solution{  
    
    public static int duplicateNumber(int arr[]) {
    	//Your code goes here
        
     for(int i=0; i<arr.length; i++){
         int count=0;
         for(int j=0; j<arr.length; j++){
             if(arr[i]==arr[j]){
                 count++;
             }
         }
        if(count==2){
            return arr[i];
        }
     }
        return arr[0];
        
        
        
    }
}
